      "$value": 200,
      "$extensions": { "mode": { "default": 200, "editorial": 200, "technical": 200 } }
    },
    "light": {
      "$type": "fontWeight",
      "$value": 300,
      "$extensions": { "mode": { "default": 300, "editorial": 300, "technical": 300 } }
    },
    "regular": {
      "$type": "fontWeight",
      "$value": 400,
      "$extensions": { "mode": { "default": 400, "editorial": 400, "technical": 400 } }
