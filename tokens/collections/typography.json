{
  "$description": "Typography collection - Font style modes: default, editorial, technical",
  "$extensions": {
    "figma.collection": "Typography",
    "figma.modes": ["default", "editorial", "technical"]
  },

  "font-family": {
    "display": {
      "$type": "fontFamily",
      "$value": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "$extensions": {
        "mode": {
          "default": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
          "editorial": "Playfair Display, Georgia, 'Times New Roman', serif",
          "technical": "JetBrains Mono, 'Fira Code', Consolas, monospace"
        }
      }
    },
    "heading": {
      "$type": "fontFamily",
      "$value": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "$extensions": {
        "mode": {
          "default": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
          "editorial": "Playfair Display, Georgia, 'Times New Roman', serif",
          "technical": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif"
        }
      }
    },
    "body": {
      "$type": "fontFamily",
      "$value": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "$extensions": {
        "mode": {
          "default": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
          "editorial": "Lora, Georgia, 'Times New Roman', serif",
          "technical": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif"
        }
      }
    },
    "mono": {
      "$type": "fontFamily",
      "$value": "JetBrains Mono, 'Fira Code', 'SF Mono', Consolas, monospace",
      "$extensions": {
        "mode": {
          "default": "JetBrains Mono, 'Fira Code', 'SF Mono', Consolas, monospace",
          "editorial": "JetBrains Mono, 'Fira Code', 'SF Mono', Consolas, monospace",
          "technical": "JetBrains Mono, 'Fira Code', 'SF Mono', Consolas, monospace"
        }
      }
    },
    "ui": {
      "$type": "fontFamily",
      "$value": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "$extensions": {
        "mode": {
          "default": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
          "editorial": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
          "technical": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif"
        }
      }
    }
